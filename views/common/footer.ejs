<style>
  /* Footer Styling */
  .footer-area {
    background: url("assets/img/gallery/section_bg02.png") no-repeat center
      center/cover;
    color: #fff;
    padding: 80px 0;
    position: relative;
  }

  /* Footer Logo */
  .footer-logo img {
    width: 180px;
    margin-bottom: 20px;
  }

  /* Footer Title */
  .footer-tittle h4 {
    font-size: 20px;
    font-weight: 600;
    color: #ffcc00;
    margin-bottom: 20px;
    position: relative;
    /* display: inline-block;s */
  }

  .footer-tittle h4::after {
    display: block;
    width: 40px;
    height: 3px;
    background: #ffcc00;
    margin-top: 5px;
  }

  /* Footer Links */
  .footer-tittle ul {
    list-style: none;
    padding: 0;
  }

  .footer-tittle ul li {
    margin-bottom: 10px;
  }

  .footer-tittle ul li a {
    color: #ddd;
    text-decoration: none;
    transition: 0.3s ease-in-out;
  }

  .footer-tittle ul li a:hover {
    color: #d414c9;
    padding-left: 5px;
  }

  /* Instagram Feed */
  .instagram-gellay ul {
    display: flex;
    flex-wrap: wrap;
    padding: 0;
    gap: 8px;
  }

  .instagram-gellay ul li {
    list-style: none;
  }

  .instagram-gellay ul li img {
    border-radius: 10px;
    transition: 0.3s ease-in-out;
  }

  .instagram-gellay ul li img:hover {
    transform: scale(1.1);
  }

  /* Social Icons */
  .footer-social {
    display: flex;
    align-items: center;
    gap: 15px;
  }

  .footer-social span {
    font-weight: 600;
    color: #ffcc00;
  }

  .footer-social a {
    color: #fff;
    font-size: 18px;
    transition: 0.3s;
  }

  .footer-social a:hover {
    color: #d414c9;
    transform: scale(1.2);
  }

  /* Footer Bottom */
  .footer-bottom {
    margin-top: 40px;
    padding-top: 20px;
    border-top: 1px solid rgba(255, 255, 255, 0.2);
    text-align: center;
  }

  .footer-bottom p {
    color: #bbb;
    font-size: 14px;
  }

  .footer-bottom p a {
    color: #d414c9;
    text-decoration: none;
  }

  .footer-bottom p a:hover {
    text-decoration: underline;
  }

  /* Responsive Design */
  @media (max-width: 768px) {
    .footer-area {
      text-align: center;
    }

    .footer-social {
      justify-content: center;
    }

    .instagram-gellay ul {
      justify-content: center;
    }
  }
</style>
